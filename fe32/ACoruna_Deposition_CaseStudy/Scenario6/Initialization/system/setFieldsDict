FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha1 0
);

regions
(
    boxToCell
    {
        box (0 0 -0.045) (9 0.02 0.1);
        fieldValues
        (
            volScalarFieldValue alpha1 1
            //volVectorFieldValue U (0.4 0 0)
        );
    }
);

