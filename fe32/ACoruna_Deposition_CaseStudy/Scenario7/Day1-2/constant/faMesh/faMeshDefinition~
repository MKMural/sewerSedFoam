FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      faMeshDefinition;
}
// ************************************************************************** //

polyMeshPatches  1( bed );

boundary
{

    inlet
    {
        type                patch;
        ownerPolyPatch      bed;
        neighbourPolyPatch  inlet;    
     }

    outlet
    {
        type                patch;
        ownerPolyPatch      bed;
        neighbourPolyPatch  outlet;    
     }

    sideWall
    {
        type                empty;
        ownerPolyPatch      bed;
        neighbourPolyPatch  sideWall;
    }
}

