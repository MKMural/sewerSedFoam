FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     interFoam;

startFrom       latestTime;

startTime       20;

stopAt          endTime;

endTime         20000;

deltaT          0.004;

writeControl    adjustableRunTime;

writeInterval   50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

adjustTimeStep  off;

maxCo           0.8;

maxDeltaT       0.05;

libs 
(
    "libgroovyBC.so"
    "libfieldFunctionObjects.so"
);

functions
{
    probes
     {
	type probes;
	outputControl outputTime;
	outputInterval 1;
	fields
	(
	  U
	);

	probeLocations
	(
	    (6 0 -0.0095)	
	    (6 0 -0.0045)
	    (6 0 0.0005)
	    (6 0 0.0055)
	    (6 0 0.0105)
	    (6 0 0.0155)
	    (6 0 0.0205)
	    (6 0 0.0255)
	    (6 0 0.0305)
	);
     }

	fieldMinMax1
	{
	    type            fieldMinMax;
	    fields          (U p k omega);
	    outputControl   outputTime;
	    writeToFile     yes;
	    log             yes;
	    location        yes;
	    mode            magnitude; 
	}
}
