FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "20";
    object      C2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    sideWall
    {
        type            empty;
    }
    bed
    {
        type            zeroGradient;
    }
    endWall
    {
        type            zeroGradient;
    }
    topWall
    {
        type            zeroGradient;
    }
}


