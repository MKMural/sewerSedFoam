FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "83";
    object      Ug;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    sideWall
    {
        type            empty;
    }
    bed
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    topWall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}


