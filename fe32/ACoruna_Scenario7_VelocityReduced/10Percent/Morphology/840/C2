{
    inlet
    {
        type            groovyBC;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 1;
        value           uniform 0;
        valueExpression "pos().z < 0.095 ? 0 : 0";
        gradientExpression "0";
        fractionExpression "1";
        evaluateDuringConstruction 0;
        variables       "";
        timelines       (
);
        lookuptables    (
);
    }
    outlet
    {
        type            zeroGradient;
    }
    sideWall
    {
        type            empty;
    }
    bed
    {
        type            zeroGradient;
    }
    endWall
    {
        type            zeroGradient;
    }
    topWall
    {
        type            zeroGradient;
    }
}


